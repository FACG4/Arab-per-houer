

     <div class="clear"></div>
            <h2 class="text--right pull--right">حسابي الشخصي</h2>
            <div class="clear"></div>

            <div class="user--profile">
                <div class="user--profile-img pull--right">
                    <img src="img/no_image_user.png" alt="">
                </div>
                <div class="user--profile-info pull--right">
                    <h3 class="clear text--right">{{details.first_name}}  {{details.last_name}}</h3>
                   <div class="user--profile-header">
                    <div class="user--profile-stars pull--right">
                        <i class="fas fa-star"></i>
                        <i class="fas fa-star"></i>
                        <i class="fas fa-star"></i>
                        <i class="fas fa-star"></i>
                        <i class="fas fa-star"></i>
                    </div>
                    <span class="pull--right">{{projectCount}}مشاريع أنشىء</span>
                    <span class="pull--right"></span>
                    <span class="pull--right">فلسطين</span>
                </div>
                    <div class="clear text--right"> <span>May 29, 2018</span> عضو منذ</div>
                    <div class="text--right"><p>{{details.position}}</p></div>
                </div>
                <div class="user--profile-btn"><button>تعديل</button></div>
                <div class="clear"></div>

            </div>
            <div class="user--projects clear">
                <h3 class="text--right pull--right ">مشاريعي</h3>
                <div class="clear"></div>
                {{#each info}}
                <div class="user--project">
                    <span class="text--right pull--right">{{this.title}}</span>
                    <span id="project-user-date"> <span>بتاريخ</span>{{this.time}}</span>
                </div>
                {{/each}}
                
            </div>